{
  "base_template": "base",
  "mimetypes": {
    "text/html": true
  },
  "preprocessors": {
    "100-pygments": {
      "type": "nbconvert.preprocessors.CSSHTMLHeaderPreprocessor",
      "enabled": true,
      "style": "default"
    },
    "200-remove-input-preprocessor": {
      "type": "pretty_jupyter.RemoveInputPreprocessor",
      "enabled": true,
      "jinja": 1
    },
    "300-remove-stdout-preprocessor": {
      "type": "pretty_jupyter.RemoveOutputPreprocessor",
      "enabled": true
    },
    "400-token-preprocessor": {
      "type": "pretty_jupyter.TokenPreprocessor",
      "enabled": true
    }
  }
}
